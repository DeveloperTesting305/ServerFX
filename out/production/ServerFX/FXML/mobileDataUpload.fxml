<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="130.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.MobileDataUpload">
   <children>
      <VBox layoutX="116.0" layoutY="-11.0" prefHeight="400.0" prefWidth="662.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox spacing="20.0">
               <children>
                  <JFXComboBox fx:id="CBselectFile" labelFloat="true" maxWidth="1.7976931348623157E308" promptText="Select File" HBox.hgrow="ALWAYS" />
                  <Button fx:id="BTcreateFile" layoutX="468.0" layoutY="10.0" mnemonicParsing="false" onAction="#createFile" text="CREATE FILE" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="30.0" layoutY="30.0" spacing="20.0">
               <children>
                  <Label fx:id="LBpath" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                  <Button fx:id="BTshowInFolder" mnemonicParsing="false" text="SHOW IN FOLDER" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" layoutX="30.0" layoutY="30.0" spacing="20.0">
               <children>
                  <ProgressBar fx:id="progressBar" maxWidth="1.7976931348623157E308" progress="0.0" HBox.hgrow="ALWAYS" />
                  <Button fx:id="BTuploadFile" layoutX="505.0" layoutY="10.0" mnemonicParsing="false" onAction="#uploadFile" text="UPLOAD FILE" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
